    <style>
        :root {
            --primary-green: #10c610;
            --primary-blue: #1a79b3;
            --primary-orange: #ff5c16;
            --primary-dark: #0a2233;
            --primary-light: #f6f9fa;
        }

        .theme-btn {
            background: var(--primary-blue) !important;
            border-color: var(--primary-blue) !important;
            color: #fff !important;
            box-shadow: 0 2px 8px 0 rgba(16, 198, 16, 0.15);
            transition: background 0.2s, border 0.2s;
        }

        /* Buttons */
        .theme-btn,
        .theme-btn.style4,
        .theme-btn.style5 {
            background: var(--primary-blue) !important;
            border-color: var(--primary-green) !important;
            color: #fff !important;
            box-shadow: 0 2px 8px 0 rgba(16, 198, 16, 0.15);
            transition: background 0.2s, border 0.2s;
        }

        .theme-btn:hover,
        .theme-btn:focus {
            background: var(--primary-blue) !important;
            border-color: var(--primary-blue) !important;
            color: #fff !important;
        }

        .theme-btn.style2 {
            background: var(--primary-orange) !important;
            border-color: var(--primary-orange) !important;
            color: #fff !important;
        }

        /* Header */
        .header-section-33,
        #header-sticky.header-1.header-3 {
            background: white !important;
            border-bottom: 3px solid var(--primary-green);
        }

        .header-logo img {
            filter: drop-shadow(0 2px 8px rgba(26, 121, 179, 0.08));
        }

        /* Navigation links */
        .main-menu ul li a {
            color: #000 !important;
            font-weight: 500;
        }

        .main-menu ul li.active>a,
        .main-menu ul li a:hover {
            color: var(--primary-orange) !important;
        }

        /* Section Titles */
        .section-title .title,
        .single-section-title {
            color: var(--primary-blue) !important;
            font-weight: 700;
        }

        .section-title .subtitle,
        .single-section-title .subtitle {
            color: black !important;
            font-weight: 600;
        }

        /* Subtitles with bg2 */
        .bg2 {
            background: var(--primary-orange) !important;
            color: #fff !important;
            border-radius: 6px;
            padding: 0.2em 0.8em;
        }

        /* Pricing Cards */
        .pricing-card.style1 {
            border: 2px solid var(--primary-blue) !important;
            box-shadow: 0 2px 16px 0 rgba(26, 121, 179, 0.08);
        }

        .pricing-card-header {
            border-radius: 10px 10px 0 0;
        }

        .pricing-card .price {
            color: var(--primary-orange) !important;
            font-weight: 700;
        }

        /* Checklist icons */
        .checklist img,
        .checklist svg {
            filter: drop-shadow(0 2px 4px rgba(16, 198, 16, 0.10));
            color: var(--primary-green) !important;
            fill: var(--primary-green) !important;
        }

        /* Footer */
        .footer-section {
            background: var(--primary-dark) !important;
            color: #fff !important;
        }

        .footer-section a {
            color: var(--primary-green) !important;
        }

        .footer-section a:hover {
            color: var(--primary-orange) !important;
        }

        .footer-bottom.style1 {
            background: var(--primary-blue) !important;
            color: #fff !important;
        }

        /* Icons */
        .fa-chevron-up,
        .fa-arrow-right-long,
        .fa-search,
        .fa-bars,
        .fa-plus,
        .fa-angle-down {
            color: var(--primary-green) !important;
        }

        .fa-facebook-f,
        .fa-twitter,
        .fa-linkedin-in,
        .fa-instagram,
        .fa-youtube {
            color: var(--primary-blue) !important;
        }

        /* Blog Cards */
        .blog-card.style1 {
            border-top: 4px solid var(--primary-orange) !important;
        }

        /* Misc */
        .back-to-top {
            color: #fff !important;
        }

        .back-to-top:hover {
            background: var(--primary-orange) !important;
        }

        .header-button a {
            background-color: #1a79b3 !important;
            transition: all 0.2s ease-in-out;
        }

        .header-button a:hover {
            background: var(--primary-orange) !important;
            border-color: var(--primary-blue) !important;
            color: #fff !important;
        }

        .subtitle {
            color: black !important;
        }

        .wow {
            color: black !important;
        }

        .fadeInUp {
            color: black !important;
        }

        .hero-image {
            width: 500px;
            height: 500px;
        }

        @media (max-width: 768px) {
            .hero-image {
                width: 100%;
                height: 100%;
            }
        }

        @media (max-width: 1200px) {
            .hero-image {
                width: 100%;
                height: 100%;
            }
        }

        /* Terms and Conditions Page Styles */
        .terms-container {
            max-width: 1200px;
            margin: 0 auto;
            padding: 80px 20px;
            background: #fff;
        }

        .terms-header {
            text-align: center;
            margin-bottom: 60px;
            padding: 40px 0;
            background: linear-gradient(135deg, var(--primary-blue) 0%, var(--primary-green) 100%);
            border-radius: 15px;
            color: white;
            box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);
        }

        .terms-header h1 {
            font-size: 2.5rem;
            margin-bottom: 15px;
            font-weight: 700;
        }

        .terms-header p {
            font-size: 1.1rem;
            opacity: 0.9;
            margin: 0;
        }

        .terms-content {
            background: #fff;
            padding: 40px;
            border-radius: 15px;
            box-shadow: 0 2px 15px rgba(0, 0, 0, 0.05);
        }

        .terms-content h2 {
            color: var(--primary-blue);
            font-size: 1.8rem;
            margin: 40px 0 20px;
            padding-bottom: 10px;
            border-bottom: 2px solid var(--primary-green);
        }

        .terms-content h3 {
            color: var(--primary-orange);
            font-size: 1.4rem;
            margin: 30px 0 15px;
        }

        .terms-content p {
            color: #444;
            line-height: 1.8;
            margin-bottom: 20px;
            font-size: 1.05rem;
        }

        .terms-content ul li strong {
            color: var(--primary-blue);
            display: block;
            margin-bottom: 5px;
        }

        .terms-content a {
            color: var(--primary-blue);
            text-decoration: none;
            transition: color 0.2s ease;
        }

        .terms-content a:hover {
            color: var(--primary-orange);
            text-decoration: underline;
        }

        .contact-info {
            background: #f8f9fa;
            padding: 30px;
            border-radius: 10px;
            margin-top: 40px;
            border: 1px solid #e9ecef;
        }

        /* Responsive Design */
        @media (max-width: 768px) {
            .terms-container {
                padding: 40px 15px;
            }

            .terms-header {
                padding: 30px 20px;
                margin-bottom: 30px;
            }

            .terms-header h1 {
                font-size: 2rem;
            }

            .terms-content {
                padding: 25px;
            }

            .terms-content h2 {
                font-size: 1.5rem;
            }

            .terms-content h3 {
                font-size: 1.2rem;
            }

            .terms-content p {
                font-size: 1rem;
            }
        }

        @media (max-width: 480px) {
            .terms-header h1 {
                font-size: 1.8rem;
            }

            .terms-content {
                padding: 20px;
            }

            .terms-content h2 {
                font-size: 1.3rem;
            }

            .terms-content h3 {
                font-size: 1.1rem;
            }
        }
    </style>
</head>

