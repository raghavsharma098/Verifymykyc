        .privacy-content {
            background: #fff;
            padding: 40px;
            border-radius: 15px;
            box-shadow: 0 2px 15px rgba(0, 0, 0, 0.05);
        }

        .privacy-content h2 {
            color: var(--primary-blue);
            font-size: 1.8rem;
            margin: 40px 0 20px;
            padding-bottom: 10px;
            border-bottom: 2px solid var(--primary-green);
        }

        .privacy-content h3 {
            color: var(--primary-orange);
            font-size: 1.4rem;
            margin: 30px 0 15px;
        }

        .privacy-content h4 {
            color: var(--primary-blue);
            font-size: 1.2rem;
