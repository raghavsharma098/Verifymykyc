    <!-- Remove the previous dropdown initialization script and replace with this -->
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Mobile menu toggle
            const sidebarToggle = document.querySelector('.sidebar__toggle');
            const headerMiddle = document.querySelector('.header-middle');
            const offcanvasOverlay = document.querySelector('.offcanvas__overlay');
            const body = document.body;

            function toggleMenu() {
                headerMiddle.classList.toggle('active');
                body.classList.toggle('menu-open');
            }

            if (sidebarToggle) {
                sidebarToggle.addEventListener('click', function(e) {
                    e.preventDefault();
                    e.stopPropagation();
                    toggleMenu();
                });
            }

            if (offcanvasOverlay) {
                offcanvasOverlay.addEventListener('click', function() {
                    toggleMenu();
                });
            }

            // Close menu when clicking outside
            document.addEventListener('click', function(e) {
                if (window.innerWidth <= 991) {
                    if (!e.target.closest('.header-middle') && 
                        !e.target.closest('.sidebar__toggle') && 
                        headerMiddle.classList.contains('active')) {
                        toggleMenu();
                    }
                }
            });

            // Handle dropdown menus
            const dropdownToggles = document.querySelectorAll('.has-dropdown > a');
            dropdownToggles.forEach(toggle => {
                toggle.addEventListener('click', function(e) {
                    if (window.innerWidth <= 991) {
                        e.preventDefault();
                        const parent = this.parentElement;
                        const submenu = parent.querySelector('.submenu');
                        if (submenu) {
                            const isOpen = submenu.style.display === 'block';
                            submenu.style.display = isOpen ? 'none' : 'block';
                        }
                    }
                });
            });

            // Handle user dropdown for mobile
            const userDropdown = document.getElementById('userDropdown');
            const dropdownMenu = userDropdown?.nextElementSibling;
            
            if (userDropdown && dropdownMenu) {
                // Toggle dropdown on click for mobile
                userDropdown.addEventListener('click', function(e) {
                    if (window.innerWidth <= 991) {
                        e.preventDefault();
                        e.stopPropagation();
                        this.classList.toggle('show');
                        dropdownMenu.classList.toggle('show');
                    }
                });

                // Close dropdown when clicking outside
                document.addEventListener('click', function(e) {
                    if (window.innerWidth <= 991) {
                        if (!userDropdown.contains(e.target) && !dropdownMenu.contains(e.target)) {
                            userDropdown.classList.remove('show');
                            dropdownMenu.classList.remove('show');
                        }
                    }
                });

                // Handle window resize
                window.addEventListener('resize', function() {
                    if (window.innerWidth > 991) {
                        userDropdown.classList.remove('show');
                        dropdownMenu.classList.remove('show');
                    }
                });

                // Close dropdown when clicking menu items
                dropdownMenu.querySelectorAll('.dropdown-item').forEach(item => {
                    item.addEventListener('click', function() {
                        if (window.innerWidth <= 991) {
                            userDropdown.classList.remove('show');
                            dropdownMenu.classList.remove('show');
                        }
                    });
                });
            }

            // Close all dropdowns when window is resized
            window.addEventListener('resize', function() {
                if (window.innerWidth > 991) {
                    const submenus = document.querySelectorAll('.submenu');
                    const dropdownMenus = document.querySelectorAll('.dropdown-menu');
                    
                    submenus.forEach(menu => {
                        menu.style.display = '';
                    });
                    
                    dropdownMenus.forEach(menu => {
                        menu.style.display = '';
                    });
                    
                    if (headerMiddle.classList.contains('active')) {
                        toggleMenu();
                    }
                }
            });
        });
    </script>

    <!-- Mobile nav dropdown toggle -->
    <script>
    document.addEventListener('DOMContentLoaded', function() {
        var mobileDropdown = document.querySelector('.mobile-dropdown');
        var dropdownToggle = document.getElementById('mobileServicesToggle');
        var dropdownMenu = document.getElementById('mobileServicesMenu');
        if (mobileDropdown && dropdownToggle && dropdownMenu) {
            dropdownToggle.addEventListener('click', function(e) {
                e.preventDefault();
                mobileDropdown.classList.toggle('open');
            });
        }
    });
