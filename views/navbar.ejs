                position: absolute;
                right: 0;
                top: 100%;
                background: white;
                z-index: 1000;
                display: none;
            }

            .dropdown-menu.show {
                display: block;
                animation: fadeInDown 0.2s ease-out;
            }

            .user-dropdown {
                width: 100%;
                justify-content: space-between;
            }

            .user-dropdown::after {
                content: '\f107';
                font-family: 'Font Awesome 6 Free';
                font-weight: 900;
                transition: transform 0.2s;
            }

            .user-dropdown.show::after {
                transform: rotate(180deg);
            }

            .dropdown-item {
                padding: 0.75rem 1rem;
                white-space: nowrap;
            }
            .hide-btn{
                display: none;
            }
